{
  "name": "update-cdn-urls",
  "version": "0.1.0",
  "description": "Confirm the integrity of CDN URLs based on installed npm packages and update their integrity attribute (HTML) and paths (JS or HTML).",
  "main": "src/index.js",
  "bin": {
    "update-cdn-urls": "./bin/index.js",
    "ucu": "./bin/index.js"
  },
  "scripts": {
    "cli": "./bin/index.js",
    "example": "./bin/index.js --file \"test/fixtures/sample.html\"",
    "example-basepath": "./bin/index.js --file \"test/fixtures/sample.html\" --cdnBasePath=\"https://unpkg.com/(?<name>[^@]*)@(?<version>\\d+\\.\\d+.\\d+)/(?<path>[^'\\\"]*)\"",
    "eslint": "eslint --ext=js,md,html .",
    "mocha": "mocha --require chai/register-expect",
    "nyc": "nyc --cache=false npm run mocha",
    "test": "npm run eslint && npm run nyc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettz9/update-cdn-urls.git"
  },
  "keywords": [
    "cdn",
    "url"
  ],
  "author": "Brett Zamir <brettz9@yahoo.com> (http://brett-zamir.me/)",
  "contributors": [],
  "license": "MIT",
  "bugs": "https://github.com/brettz9/update-cdn-urls/issues",
  "homepage": "https://github.com/brettz9/update-cdn-urls#readme",
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "command-line-basics": "^0.8.0",
    "globby": "^11.0.1"
  },
  "devDependencies": {
    "@mysticatea/eslint-plugin": "^13.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.4.0",
    "eslint-config-ash-nazg": "^22.5.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-array-func": "^3.1.6",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^29.2.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-no-unsanitized": "^3.1.2",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^20.1.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "typescript": "^3.9.6"
  }
}
